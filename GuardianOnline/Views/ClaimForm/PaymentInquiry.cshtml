<fieldset class="scheduler-border" @*style="background-color:lightgray"*@ id="Expended_services">
    <legend class="scheduler-border" style="color:teal">Payment Inquiry Data</legend>
    <div id="claim_services2">
        @*table7899*@
        <table class="table table-dark table-hover" id="Claim_Services2" cellpadding="0" cellspacing="0">
            <thead>
                <tr>
                    <th style="background-color:teal">Provider Name</th>
                    <th style="background-color:teal">Batch Number</th>
                    <th style="background-color:teal">Rec Date</th>
                    <th style="background-color:teal">Due Date</th>
                    <th style="background-color:teal">Payment Date</th>
                    <th style="background-color:teal">Rec Batch Amt</th>
                    <th style="background-color:teal">Net Payment Amt.</th>
                    <th style="background-color:teal">W.H.T</th>
                    <th style="background-color:teal">Admin fees</th>
                    <th style="background-color:teal">Due Amt</th>
                    <th style="background-color:teal">Paid Amt</th>
                    <th style="background-color:teal">Rest Amt</th>
                    <th style="background-color:teal">Pay. Doc. No.</th>
                    <th style="background-color:teal">Payment Method Name</th>
                    <th style="background-color:teal">Batch Status Name</th>
                    <th style="background-color:teal">Show</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in @ViewBag.services)
                {
                    <tr>
                        <td>@item.GetType().GetProperty("ProviderName").GetValue(item)</td>
                        <td>@item.GetType().GetProperty("BatchNumber").GetValue(item)</td>
                        <td>@item.GetType().GetProperty("RecDate").GetValue(item).ToString("yyyy-MM-dd")</td>
                        <td>@item.GetType().GetProperty("DueDate").GetValue(item).ToString("yyyy-MM-dd")</td>
                        <td>@item.GetType().GetProperty("PaymentDate").GetValue(item).ToString("yyyy-MM-dd")</td>
                        <td>@item.GetType().GetProperty("RecBatchAmt").GetValue(item)</td>
                        <td>@item.GetType().GetProperty("Expr2").GetValue(item)</td>
                        <td>@item.GetType().GetProperty("WHTaxAmt").GetValue(item)</td>
                        <td>@item.GetType().GetProperty("AdminFeesAmt").GetValue(item)</td>
                        <td>@item.GetType().GetProperty("PaymentAmt").GetValue(item)</td>
                        <td>@item.GetType().GetProperty("DueNetPaymentAmt").GetValue(item)</td>
                        <td>@item.GetType().GetProperty("rest").GetValue(item)</td>
                        <td>@item.GetType().GetProperty("PayamentID").GetValue(item)</td>
                        <td>@item.GetType().GetProperty("PaymentMethodName").GetValue(item)</td>
                        @*<p class="status delivered">Delivered</p>*@
                        <td>
                            @if (@item.GetType().GetProperty("BatchStatusName").GetValue(item) == "UnPaid")
                            {
                                <p class="status UnPaid">
                                    @item.GetType().GetProperty("BatchStatusName").GetValue(item)
                                </p>
                            }
                            else if (@item.GetType().GetProperty("BatchStatusName").GetValue(item) == "Paid")
                            {
                                <p class="status Paid">
                                    @item.GetType().GetProperty("BatchStatusName").GetValue(item)
                                </p>
                            }
                            else if (@item.GetType().GetProperty("BatchStatusName").GetValue(item) == "Partly Paid")
                            {
                                <p class="status PartlyPaid">
                                    @item.GetType().GetProperty("BatchStatusName").GetValue(item)
                                </p>
                            }
                            else
                            {
                                <p class="status Received">
                                    @item.GetType().GetProperty("BatchStatusName").GetValue(item)
                                </p>
                            }


                        </td>
                        <td>
                            <a onclick='show(this);'>
                                <span class="glyphicon glyphicon-eye-open" style="color:#20c5ff"></span>
                                @*#20c5ff*@
                            </a>
                        </td>
                        @*<td><button type='button' class='btn btn-warning' data-bs-toggle='modal' data-bs-target='#myModal' style='text-align:center' onclick='show(this);'>Show</button></td>*@
                    </tr>
                }
            </tbody>
        </table>


    </div>
    <br />
    <div style="text-align:left;display:inline-block;position:relative">
        <a id="link" class="round" href="@Url.Action("DoctorHome","ClaimForm")">@*<u>*@&laquo; Back to Main Home@*</u>*@</a>
